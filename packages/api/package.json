{
  "name": "@plugin-builder/api",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "tsdown": "^0.15.5",
    "@every-plugin/template": "file:../../plugins/_plugin_template"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/client": "^1.10.0",
    "@orpc/openapi": "^1.10.0",
    "@orpc/server": "^1.10.0",
    "@orpc/zod": "^1.10.0",
    "dotenv": "^17.2.2",
    "every-plugin": "0.3.2",
    "next": "16.0.0",
    "zod": "^4.1.12"
  }
}
